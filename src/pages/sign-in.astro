---
// Redirect to Clerk Account Portal for sign-in
const signInUrl = import.meta.env.PUBLIC_CLERK_SIGN_IN_URL || "https://accounts.jobnagringa.com.br/sign-in";
const redirectUrl = Astro.url.searchParams.get("redirect_url") || "/";

return Astro.redirect(`${signInUrl}?redirect_url=${encodeURIComponent(redirectUrl)}`);
---
